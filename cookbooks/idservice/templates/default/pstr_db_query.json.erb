{
  "Name": "pstr_db_query",
  "ClassName": "org.qbroker.persister.JDBCPersister",
<% if @db_type == 'postgresql' %>
  "URI": "jdbc:postgresql://localhost:5432/<%= @db_name %>",
<% else %>
  "URI": "jdbc:mysql://localhost:3306/<%= @db_name %>",
  "MaxIdleTime": "28500",
<% end %>
  "LinkName": "query",
  "Operation": "query",
  "Username": "<%= @db_user %>",
  "Password": "<%= @db_passwd %>",
  "ResultType": "8",
  "DisplayMask": "6",
  "StringProperty": {
    "SQL": "",
    "TID": "",
    "ReturnCode": ""
  }
}
