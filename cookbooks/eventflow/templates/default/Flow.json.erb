{
  "Name": "Flow",
  "Site": "DEVOPS",
  "Category": "<%= @id %>",
  "Type": "QFlow",
  "Description": "a flow to process events",
  "Operation": "query",
  "LogDir": "<%= @log_dir %>",
  "LogDatePattern": "'.'ww",
  "ConfigDir": "<%= @qb_dir %>/flow/<%= @id %>",
  "Heartbeat": "300",
  "XAMode": "1",
  "Capacity": "1024",
  "Mode": "daemon",
  "Debug": "5",
  "FlowDebug": "37",
  "EscalationCapacity": "64",
  "MaxNumberFlow": "1",
  "OpenSSLPlugin": "org.qbroker.security.Decryptor",
  "IncludePolicy": {
    "org.qbroker.node.EventCorrelator": "Ruleset",
    "org.qbroker.node.EventMonitor": "Ruleset",
    "org.qbroker.node.EventDispatcher": "Ruleset"
  },
  "Reporter": [
    "rpt_global_var"
  ],
  "Receiver": [
    "rcvr_event_log"
  ],
  "Node": [
    "node_parser",
    "node_switch",
    "node_screen",
    "node_data_parser",
    "node_format",
    "node_correlation",
    "node_monitor",
    "node_dispatcher",
    "node_json_format",
    "node_router"
  ],
  "Persister": [
    "pstr_pool",
    "pstr_pool",
    "pstr_pool",
    "pstr_elastic",
    "pstr_null",
    "pstr_failure",
    "pstr_nohit"
  ]
}
